#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/keremdemirer/Downloads/personalizer-spec/personalizer/packages/core/src/node_modules:/Users/keremdemirer/Downloads/personalizer-spec/personalizer/packages/core/node_modules:/Users/keremdemirer/Downloads/personalizer-spec/personalizer/packages/node_modules:/Users/keremdemirer/Downloads/personalizer-spec/personalizer/node_modules:/Users/keremdemirer/Downloads/personalizer-spec/node_modules:/Users/keremdemirer/Downloads/node_modules:/Users/keremdemirer/node_modules:/Users/node_modules:/node_modules:/Users/keremdemirer/Downloads/personalizer-spec/personalizer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/keremdemirer/Downloads/personalizer-spec/personalizer/packages/core/src/node_modules:/Users/keremdemirer/Downloads/personalizer-spec/personalizer/packages/core/node_modules:/Users/keremdemirer/Downloads/personalizer-spec/personalizer/packages/node_modules:/Users/keremdemirer/Downloads/personalizer-spec/personalizer/node_modules:/Users/keremdemirer/Downloads/personalizer-spec/node_modules:/Users/keremdemirer/Downloads/node_modules:/Users/keremdemirer/node_modules:/Users/node_modules:/node_modules:/Users/keremdemirer/Downloads/personalizer-spec/personalizer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  --import tsx "$basedir/../@personalizer/core/src/cli.ts" "$@"
else
  exec node  --import tsx "$basedir/../@personalizer/core/src/cli.ts" "$@"
fi
